    "import pandas\n",
    "import seaborn as sns\n",
    "import numpy as np\n",
    "import os\n",
    "import matplotlib.pyplot as plt\n",
    "names = ['PF1C','PF5','PF11B','PF12','PF14','PF15C','PF15D','IF1B','IF4','IF5']\n",
    "#data = pandas.read_table('matrix.txt',sep='\\s+')  ,
    "#correlations = data.corr(method='spearman') \n",
    "#correction=abs(correlations)\n",
    "\n",
   
    "data = np.genfromtxt(\"matrix 3 combine 1.csv\", delimiter=\",\")\n",
    "matrix = data[1:,1:11]\n",
    "\n",
    "# plot correlation matrix \n",
    "fig, ax = plt.subplots(figsize = (5,5))\n",
    "ax.set_xticklabels(matrix,rotation='horizontal')\n",
    "sns.heatmap(matrix, cmap=plt.cm.Reds, linewidths=0.1, vmax=1, vmin=0 ,annot=False, square = True,fmt='.2g')\n",
    "\n",
    "#热力图参数设置（相关系数矩阵，颜色，每个值间隔等）\n",
    "#ticks = numpy.arange(0,16,1) #生成0-16，步长为1 \n",
    "#plt.xticks(np.arange(25),names) #横坐标标注点\n",
    "#plt.yticks(np.arange(25),names) #纵坐标标注点\n",
    "#ax.set_xticks(ticks) #生成刻度 \n",
    "#ax.set_yticks(ticks)\n",
    "ax.set_xticklabels(names,fontsize = 10, horizontalalignment='center',rotation = 45) \n",
    "ax.set_yticklabels(names, fontsize = 10, horizontalalignment='right',rotation = 360)\n",
    "ax.tick_params(axis='both', which='both', length=0)\n",
    "#ax.set_title('Characteristic correlation')\n",
    "#plt.savefig('cluster.tif',dpi=300)\n",
    "plt.show()"
   ]
